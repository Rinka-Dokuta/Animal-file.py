#Writing animal files

file = open("Animals.txt", 'w')
file.write("Lion \n")
file.write("Tiger \n")
file.write("Jaguars\n")
file.close()

#Reading from a file
file = open('Animals.txt', 'r')
content = file.read()
print(content)
file.close

#Appending to a file
file = open('Animals.txt','a')
file.write("Cheetah \n")
file.close()

#Reading the appended content
file = open('example.txt', 'r')
content = file.read()
print(content)
file.close
